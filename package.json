{
  "name": "wick",
  "version": "0.0.31",
  "description": "PWA Framework",
  "main": "build/wick-min-es.js",
  "scripts": {
    "test": "mocha",
    "test-n-watch": "mocha -w --check-leaks -R min",

    "build-production": "npx rollup --config ./build_config/minibuild.config.js",
    "build-production-es": "npx rollup --config ./build_config/minibuildes.config.js",

    "build-dev": "npx rollup --config ./build_config/browserdev.config.js",
    "build-node": "npx rollup --config ./build_config/node.config.js",

    "watch": "npx rollup --config ./build_config/browserdev.config.js --watch",
    "watch-node": "npx rollup --config ./build_config/node.config.js --watch",

    "docs": "npx jsdoc ./source/ --destination ./documents/api --configure ./build_config/jsdoc.json "
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/anthonycweathersby/wick.git"
  },
  "keywords": [
    "SPA",
    "PWA",
    "Framework",
    "JavaScript",
    "Animation",
    "Forms",
    "Website",
    "REST",
    "APP"
  ],
  "author": "Anthony Weathersby",
  "license": "copyright",
  "bugs": {
    "url": "https://gitlab.com/anthonycweathersby/wick/issues"
  },
  "private": true,
  "homepage": "https://gitlab.com/anthonycweathersby/wick#readme",
  "devDependencies": {
    "jsdocs": "^1.0.0",
    "jsdom": "^11.11.0",
    "mocha": "^5.2.0",
    "rollup": "^0.62.0",
    "rollup-plugin-terser": "^1.0.1"
  },
  "optionalDependencies": {},
  "dependencies": {}
}
