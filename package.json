{
  "name": "wick",
  "version": "0.0.2",
  "description": "PWA Framework",
  "main": "build/wick.js",
  "scripts": {
    "test": "mocha",
    "rollup" : "npx rollup ./source/wick.js --format iife --name \"wick\" --file build/wick.js",
    "build": "webpack --config ./wp-config/build.config.js",
    "dev-build": "webpack --config ./wp-config/build-dev.config.js",
    "watch": "webpack --config ./wp-config/watch.config.js",
    "watch-es5": "webpack --config ./wp-config/watches5.config.js",
    "docs": "jsdoc ./source/ --destination ./documents/api --configure ./wp-config/jsdoc.json "
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/anthonycweathersby/wick.git"
  },
  "keywords": [
    "SPA",
    "Framework",
    "JavaScript",
    "Animation",
    "Forms",
    "Website",
    "REST"
  ],
  "author": "Anthony Weathersby",
  "license": "copyright",
  "bugs": {
    "url": "https://gitlab.com/anthonycweathersby/wick/issues"
  },
  "private": true,
  "homepage": "https://gitlab.com/anthonycweathersby/wick#readme",
  "devDependencies": {
    "jsdom": "^11.11.0",
    "mocha": "^5.2.0",
    "rollup": "^0.62.0"
  },
  "optionalDependencies": {
    "jsdoc-webpack-plugin": "0.0.2",
    "jsdoc-webpack-plugin-v2": "^2.0.2",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.10.1",
    "webpack-cli": "^2.1.4"
  },
  "dependencies": {}
}
